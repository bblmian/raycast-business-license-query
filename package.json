{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "business-information-query",
  "title": "Business Information Query",
  "description": "Query and verify business license information",
  "icon": "command-icon.png",
  "author": "zhubo",
  "categories": [
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Business Info Search",
      "description": "Search and verify business information",
      "mode": "view",
      "icon": "command-icon.png"
    }
  ],
  "preferences": [
    {
      "name": "apiKey",
      "type": "password",
      "required": true,
      "title": "API Key",
      "description": "Your Baidu API Key"
    },
    {
      "name": "secretKey",
      "type": "password",
      "required": true,
      "title": "Secret Key",
      "description": "Your Baidu Secret Key"
    },
    {
      "name": "requestInterval",
      "type": "textfield",
      "required": false,
      "title": "Request Interval (ms)",
      "description": "Interval between API requests in milliseconds",
      "default": "1000"
    },
    {
      "name": "maxConcurrent",
      "type": "textfield",
      "required": false,
      "title": "Max Concurrent Requests",
      "description": "Maximum number of concurrent API requests",
      "default": "5"
    },
    {
      "name": "batchSize",
      "type": "textfield",
      "required": false,
      "title": "Batch Size",
      "description": "Number of items to process in each batch",
      "default": "20"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/archiver": "^5.3.4",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
